{
    "name": "scss-autocomplete",
    "displayName": "SCSS Autocomplete",
    "description": "Autocomplete and refactoring support for SCSS",
    "version": "0.0.1",
    "publisher": "Alex",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages",
        "Language Packs"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "keywords": [
        "SCSS",
        "SCSS Autocomplete",
        "SCSS Intellisense"
    ],
    "main": "./out/extension",
    "preview": true,
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}